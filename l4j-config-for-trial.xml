<launch4jConfig>
    <dontWrapJar>true</dontWrapJar>
    <headerType>gui</headerType>
    <jar>Wezzle.jar</jar>
    <outfile>dist/Wezzle.exe</outfile>
    <errTitle>Wezzle Error</errTitle>
    <cmdLine>--trial</cmdLine>
    <chdir></chdir>
    <priority>normal</priority>
    <downloadUrl>http://java.com/download</downloadUrl>
    <supportUrl>http://couchware.ca/www/wezzle</supportUrl>
    <customProcName>true</customProcName>
    <stayAlive>false</stayAlive>
    <manifest></manifest>
    <icon>src/resources/wezzle.ico</icon>
    <jre>
        <path></path>
        <minVersion>1.5.0_05</minVersion>
        <maxVersion></maxVersion>
        <jdkPreference>preferJre</jdkPreference>
        <opt>-ea -Djava.library.path=lib/native/windows -Dsun.java2d.noddraw=true -Dsun.awt.noerasebackground=true -Dsun.java2d.d3d=false -Dsun.java2d.opengl=false -Dsun.java2d.pmoffscreen=false</opt>
    </jre>
    <versionInfo>
        <fileVersion>1.3.4.0</fileVersion>
        <txtFileVersion>1.3.4</txtFileVersion>
        <fileDescription>Wezzle is a fast-paced puzzle game</fileDescription>
        <copyright>Copyright 2008-2010 Couchware Inc.</copyright>
        <productVersion>1.3.4.0</productVersion>
        <txtProductVersion>1.3.4</txtProductVersion>
        <productName>Wezzle</productName>
        <companyName>Couchware Inc.</companyName>
        <internalName>Wezzle</internalName>
        <originalFilename>Wezzle.exe</originalFilename>
    </versionInfo>
</launch4jConfig>